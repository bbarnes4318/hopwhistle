{
  "name": "@hopwhistle/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:demo": "tsx prisma/demo-seed.ts",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "recordings:backfill": "tsx src/cli/backfill-recordings.ts",
    "recordings:backfill:one": "tsx src/cli/backfill-recordings.ts --recording-id",
    "dnc:import": "tsx src/cli/import-dnc.ts",
    "numbers:import": "tsx src/cli/numbers-import.ts",
    "numbers:assign": "tsx src/cli/numbers-assign.ts",
    "numbers:audit": "tsx src/cli/numbers-audit.ts",
    "quota:test": "tsx src/cli/test-quota-overage.ts",
    "migrate": "tsx src/cli/migrate.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@clickhouse/client": "^0.2.7",
    "@fastify/cookie": "^9.1.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.2.0",
    "@fastify/websocket": "^8.3.0",
    "@hopwhistle/routing-dsl": "workspace:*",
    "@hopwhistle/shared": "workspace:*",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.0",
    "@opentelemetry/exporter-jaeger": "^1.25.1",
    "@opentelemetry/exporter-prometheus": "^0.51.1",
    "@opentelemetry/instrumentation": "^0.51.1",
    "@opentelemetry/instrumentation-fastify": "^0.53.0",
    "@opentelemetry/instrumentation-http": "^0.51.1",
    "@opentelemetry/resources": "^1.24.1",
    "@opentelemetry/sdk-node": "^0.51.1",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "csv-parse": "^5.5.3",
    "decimal.js": "^10.4.3",
    "dotenv-flow": "^4.1.0",
    "fastify": "^4.25.2",
    "ioredis": "^5.3.2",
    "js-yaml": "^4.1.0",
    "modesl": "^1.1.3",
    "openapi-typescript": "^7.10.1",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "prom-client": "^15.1.0",
    "puppeteer": "^21.6.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.2",
    "prisma": "^5.7.1",
    "prisma-erd-generator": "^1.10.0",
    "supertest": "^6.3.3",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
