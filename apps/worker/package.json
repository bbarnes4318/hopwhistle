{
  "name": "@hopwhistle/worker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "simulate:calls": "tsx src/scripts/simulate-calls.ts"
  },
  "dependencies": {
    "@clickhouse/client": "^0.2.7",
    "@hopwhistle/shared": "workspace:*",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.0",
    "@opentelemetry/exporter-jaeger": "^1.25.1",
    "@opentelemetry/exporter-prometheus": "^0.51.1",
    "@opentelemetry/instrumentation": "^0.51.1",
    "@opentelemetry/instrumentation-http": "^0.51.1",
    "@opentelemetry/resources": "^1.24.1",
    "@opentelemetry/sdk-node": "^0.51.1",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@prisma/client": "^5.7.1",
    "decimal.js": "^10.4.3",
    "dotenv-flow": "^4.1.0",
    "ioredis": "^5.3.2",
    "modesl": "^1.1.3",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "prom-client": "^15.1.0",
    "puppeteer": "^21.6.1",
    "stripe": "^14.21.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
