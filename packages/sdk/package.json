{
  "name": "@hopwhistle/sdk",
  "version": "0.1.0",
  "private": false,
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "generate:types": "tsx scripts/generate-types.ts",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@hopwhistle/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.5",
    "js-yaml": "^4.1.0",
    "openapi-typescript": "^7.10.1",
    "tsup": "^8.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^1.1.0"
  }
}
